{
  "profiles": {
    "tap (Install Editor CE)": {
      "commandName": "Project",
      "commandLineArgs": "package install \"Editor CE\" --no-dependencies",
      "environmentVariables": {
        "OPENTAP_COLOR": "1"
      }
    },
    "tap (list)": {
      "commandName": "Project",
      "commandLineArgs": "package list -v"
    },
    "tap (create)": {
      "commandName": "Project",
      "commandLineArgs": "package create ..\\..\\opentap.x64.package.xml --install --ignore-missing-plugins"
    },
    "tap (gitversion)": {
      "commandName": "Project",
      "commandLineArgs": "sdk gitversion --dir c:\\git\\tapMerge\\ --log 20"
    },
    "tap (commands)": {
      "commandName": "Project"
    }
  }
}