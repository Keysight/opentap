{
  "profiles": {
    "tap (Install Editor CE)": {
      "commandName": "Project",
      "commandLineArgs": "package install \"Editor CE\" \"WPF Controls\" --no-dependencies --force",
      "environmentVariables": {
        "OPENTAP_COLOR": "auto"
      }
    },
    "tap (Install vssdk)": {
      "commandName": "Project",
      "commandLineArgs": "package install \"Visual Studio SDK CE\" --force --unpack-only",
      "environmentVariables": {
        "OPENTAP_COLOR": "auto"
      }
    },
    "tap (list)": {
      "commandName": "Project",
      "commandLineArgs": "package list -v"
    },
    "tap (create)": {
      "commandName": "Project",
      "commandLineArgs": "package create ..\\..\\opentap.x64.package.xml -v"
    },
    "tap (gitversion)": {
      "commandName": "Project",
      "commandLineArgs": "sdk gitversion --dir c:\\git\\tapMerge\\ --log 20"
    },
    "tap (commands)": {
      "commandName": "Project"
    },
    "tap (editor)": {
      "commandName": "Project",
      "commandLineArgs": "editor"
    }
  }
}